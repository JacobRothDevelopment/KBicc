  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Experimental",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\experimental\\main.js"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Experimental Test",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\experimental\\tests.js"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Experimental LargeTests JS",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\largeTest\\js\\main.js"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "JS Main",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\js\\test.js"
    }
  ]
}
